{
  "name": "nodejs-api",
  "version": "0.1.1",
  "engines": {
    "node": "12.16.x"
  },
  "description": "Node.js server",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "set NODE_ENV=dev && nodemon app.js",
    "prod": "set NODE_ENV=prod && node app.js",
    "watch": "npm run lint && npm run dev",
    "test": "istanbul cover test.js",
    "integration": "node test.js",
    "lint": "eslint ./",
    "coverage": "npm run lint && nyc npm run integration"
  },
  "keywords": [
    "node",
    "api",
    "express"
  ],
  "dependencies": {
    "async": "^2.5.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "bcryptjs-then": "^1.0.1",
    "body-parser": "^1.16.1",
    "compression": "^1.7.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "mockgoose": "^8.0.4",
    "moment": "^2.22.2",
    "mongoose": "^5.9.7",
    "node-code-generator": "^1.0.3",
    "winston": "^3.0.0-rc1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^5.2.1",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.1",
    "nodemon": "^1.12.0"
  }
}
